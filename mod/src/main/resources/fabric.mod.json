{
  "schemaVersion": 1,
  "id": "facemod",
  "version": "${version}",

  "name": "FaceMod",
  "description": "",
  "authors": [],
  "contact": {},

  "license": "MIT",
  "icon": "assets/facemod/icon.png",

  "environment": "client",
  "entrypoints": {
    "fabric-datagen": ["dev.shiftsad.facemod.client.FacemodDataGenerator"],
    "client": ["dev.shiftsad.facemod.client.FacemodClient"],
    "main": ["dev.shiftsad.facemod.Facemod"]
